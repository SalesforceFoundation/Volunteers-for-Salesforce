<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <description>Custom settings used by Volunteers for Salesforce.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Contact_Match_Email_Fields__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>A semicolon separated list of additional Contact email fields to try to match against when doing Contact matching. Note that the fields are specified by their full API Name, including namespace prefix. NPSP email fields are automatically checked.</inlineHelpText>
        <label>Contact Match Email Fields</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Contact_Match_First_Name_Fields__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>A semicolon separated list of additional Contact first name fields to try to match against when doing Contact matching. E.g., a nickname field. Note that the fields are specified by their full API Name, including namespace prefix.</inlineHelpText>
        <label>Contact Match First Name Fields</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Contact_Matching_Rule__c</fullName>
        <defaultValue>&quot;Firstname;Lastname;Email&quot;</defaultValue>
        <description>Determines which fields (FirstName, LastName, and/or Email) V4S uses when matching Contacts against existing Contacts on Volunteers Sites pages. Use a semicolon-separated list, for example: FirstName;Email. Specify fields by their API Name.</description>
        <externalId>false</externalId>
        <inlineHelpText>Determines which fields (FirstName, LastName, and/or Email) V4S uses when matching Contacts against existing Contacts on Volunteers Sites pages. Use a semicolon-separated list, for example: FirstName;Email. Specify fields by their API Name.</inlineHelpText>
        <label>Contact Matching Rule</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Google_Maps_API_Key__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>If you enter a Google Maps API Key, V4S includes a map of the Job Location on the Job Listing pages of your Volunteer Site.</inlineHelpText>
        <label>Google Maps API Key</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
