        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Personal_Site_Report_Hours_Filtered__c</fullName>
        <defaultValue>false</defaultValue>
